<div class="contents_row">
  <p class="all-users">ユーザー 一覧</p>
  <% @users.each do |user| %>
    <div class="user-info">
      <img src=<%= user.image %> class = "icon_image">
      <div class="right-info">
        <%= link_to user.nickname, about_user_path(user) %>
      </div>
    </div>
  <% end %>
</div>